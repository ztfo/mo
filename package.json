{
  "name": "mo-plugin-simple",
  "version": "1.0.0",
  "publisher": "luispalomares-dev",
  "displayName": "Mo - Simple",
  "description": "Simple version of Mo plugin with improved compatibility",
  "main": "simple-extension.js",
  "engines": {
    "vscode": "^1.60.0",
    "cursor": ">=0.2.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:mo-plugin.showTaskQueue",
    "onCommand:mo-plugin.showLinearSync",
    "onCommand:mo-plugin.showSettings",
    "onCommand:mo-plugin.planProject",
    "onCommand:mo-plugin.pushTasks",
    "onCommand:mo-plugin.showExportDialog",
    "onCommand:mo-plugin.syncWithLinear"
  ],
  "contributes": {
    "commands": [
      {
        "command": "mo-plugin.showTaskQueue",
        "title": "Mo: Show Task Queue"
      },
      {
        "command": "mo-plugin.showLinearSync",
        "title": "Mo: Show Linear Sync"
      },
      {
        "command": "mo-plugin.showSettings",
        "title": "Mo: Show Settings"
      },
      {
        "command": "mo-plugin.planProject",
        "title": "Mo: Plan Project"
      },
      {
        "command": "mo-plugin.pushTasks",
        "title": "Mo: Push Tasks to Linear"
      },
      {
        "command": "mo-plugin.showExportDialog",
        "title": "Mo: Show Export Dialog"
      },
      {
        "command": "mo-plugin.syncWithLinear",
        "title": "Mo: Sync with Linear"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "mo-plugin.showTaskQueue"
        },
        {
          "command": "mo-plugin.showLinearSync"
        },
        {
          "command": "mo-plugin.showSettings"
        },
        {
          "command": "mo-plugin.planProject"
        },
        {
          "command": "mo-plugin.pushTasks"
        },
        {
          "command": "mo-plugin.showExportDialog"
        },
        {
          "command": "mo-plugin.syncWithLinear"
        }
      ]
    }
  },
  "license": "ISC"
} 